server {
    listen 4200;

    root /Kalender/Website/frontend/dist/frontend;  # Direkt auf das dist-Verzeichnis setzen
    index index.html;
    
    location / {
        try_files $uri /index.html;
    }

    location /assets/ {
        expires 1y;
        access_log off;
    }

    error_page 404 /index.html;
}
